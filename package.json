{
  "name": "@ing3kth/core",
  "version": "3.0.0",
  "description": "@ing3kth Core for node package",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "types": "npx tsc --project tsconfig.json",
    "format": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write .",
    "lint": "npx eslint src/**",
    "prepublishOnly": "npm run format && npm run types"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/KTNG-3/npm-core.git"
  },
  "keywords": [
    "ing3kth",
    "core"
  ],
  "author": "ing3kth (https://github.com/KTNG-3)",
  "contributors": [
    "ING3_ (https://github.com/KTNG-3)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KTNG-3/npm-core/issues"
  },
  "homepage": "https://github.com/KTNG-3/npm-core#readme",
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "typescript": "^4.9.4"
  },
  "directories": {
    "test": "test"
  }
}
